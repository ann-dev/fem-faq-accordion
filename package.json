{
  "name": "fem-faq-accordion",
  "version": "1.1.2",
  "description": "Responsive design challenge solution",
  "main": "index.js",
  "scripts": {
    "watch:pug": "npx pug-cli -w . -o . -P",
    "watch:css": "npx stylus -w src/styles/stylus/index.styl -o src/styles/css/",
    "browser-sync": "browser-sync start --server src --files 'src/*.html, src/styles/css/*.css' --port 3000",
    "dev": "concurrently \"npm run watch:pug\" \"npm run watch:css\" \"npm run browser-sync\"",
    "build": "npx pug-cli . -o . -P && stylus src/styles/stylus/index.styl -o src/styles/css/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ann-dev/fem-faq-accordion.git"
  },
  "author": "ann-dev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ann-dev/fem-faq-accordion/issues"
  },
  "homepage": "https://github.com/ann-dev/fem-faq-accordion#readme",
  "dependencies": {
    "pug": "^3.0.3",
    "stylus": "^0.64.0"
  },
  "devDependencies": {
    "browser-sync": "^3.0.4",
    "concurrently": "^9.2.1",
    "pug-cli": "^1.0.0-alpha6"
  }
}
